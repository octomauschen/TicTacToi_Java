		System.out.print(a[2][0]+" "+a[2][1]+" "+a[2][2]+"\n"+"\n");
        
        System.out.println("Name for [0][0]");
        a[2][2] = scanner.nextLine();
        
        
		System.out.print(a[0][0]+" "+a[0][1]+" "+a[0][2]+"\n");
		System.out.print(a[1][0]+" "+a[1][1]+" "+a[1][2]+"\n");
		System.out.print(a[2][0]+" "+a[2][1]+" "+a[2][2]+"\n"+"\n");
        
        System.out.println("Name for [0][0]");
